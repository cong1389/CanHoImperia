<!----BannerTopOfNewsPage-->

@using App.Aplication
@model App.Domain.Entities.Ads.Banner
@if (Model != null)
{
    <section id="title-wrapper" style="background-image: url('@(Model.ImgPath.ValidateImage())');">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="title-holder">
                        <div class="title-holder-cell text-left">
                            <h2 class="page-title"><span class="text-uppercase">@ViewBag.Title</span></h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
}
<!----/BannerTopOfNewsPage-->