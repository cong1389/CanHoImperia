<!---GetStaticContent---->
@using App.Domain.Common
@model App.Domain.Entities.Data.StaticContent
@if (Model != null)
{
    @Html.Action("BannerTop", "Banner", new { menuId = ViewBag.BannerId, title = ViewBag.Title })
	@Html.Partial("_BreadCrumb")

    if (ViewBag.TemplateType == (int)TemplateContent.FixItem)
    {
        @Html.Action("GetStaticContentParent", "Menu", new { title = Model.Title, menuId = Model.MenuId, virtualId = Model.MenuLink.VirtualId })
    }
    else
    {
        <div class="mkdf-container">
            <div class="mkdf-container-inner clearfix" id="columns">
               

                <div class="center_column col-xs-12 col-sm-12" id="center_column">
                    <!-- page heading-->
                    <h2 class="page-heading">
                        <span class="page-heading-title2">@ViewBag.Title</span>
                    </h2>
                    <!-- Content page -->
                    <div class="content-text clearfix">
                        @Html.Raw(Model.Description)
                    </div>
                    <!-- ./Content page -->
                </div>
            </div>
        </div>
        
    }
}
<!---/GetStaticContent---->